Ave Imperator, morituri te salutant.

integer principalis(vacuum)
{
    integer MARCUS;
    integer LUCIUS EST X;

    MARCUS EST V;

    scribere("MARCUS est V");
    scribere(MARCUS);

    scribere("LUCIUS est X");
    scribere(LUCIUS);

    scribere(MARCUS + LUCIUS);

    MARCUS EST IX;

    scribere(MARCUS);

    reditus MARCUS + LUCIUS;
}
